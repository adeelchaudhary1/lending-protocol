{
  "name": "lending-protocol",
  "version": "1.0.0",
  "description": "A simple lending protocol.",
  "scripts": {
    "prettier": "prettier --write 'contracts/**/*.sol'",
    "compile": "npm run prettier && npx hardhat compile",
    "deploy:goerli": "npx hardhat run --network goerli scripts/deploy.js",
    "deploy:hardhat": "npx hardhat run --network hardhat scripts/deploy.js",
    "test": "npx hardhat test --network hardhat"
  },
  
  "repository": {
    "type": "git"
  },

  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.7",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "chai": "^4.3.7",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "bn-chai": "^1.0.1",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.5",
    "hardhat-gas-reporter": "^1.0.9",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "ethereum-waffle": "^3.4.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.6.1",
    "@nomiclabs/hardhat-waffle": "^2.0.5",
    "dotenv": "^16.0.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "hardhat-gas-reporter": "^1.0.8"
  }
}
